#ifndef _STRUCTS_H_
#define _STRUCTS_H_
struct EmployeeData
{
   string Eid;
   string name;
   double hours;
   double wage;
   double gross;
   double FedTax;
   double SocSec;
   double netpay;
};

struct SummaryData
{
   double Gross;
   double FedTax;
   double SocSec;
   double netPay;
};
#endif
